<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Group 3</title>
    <link rel="stylesheet" href="~/css/main.css" />
    <link
      rel="icon"
      type="image/x-icon"
      href="https://drive.google.com/uc?export=view&id=1NSUoxeb29qUtJuw3QkOa-OBE75ATv0uQ"
    />

    <!--- CDN BOOTSTRAP ---->
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.2/font/bootstrap-icons.css"
    />

    <!--- GOOGLE FONTS ---->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Herr+Von+Muellerhoff&family=Montserrat&family=Poppins:wght@700&display=swap"
      rel="stylesheet"
    />

    <!--- GOOGLE ICONS ---->
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200"
    />
  </head>
  <body>
    <!--- My Loading Screen ---->
    <div class="preloader">
      <div class="preloader-wrapper">
        <div class="preloader-circle-1"></div>
      </div>
      <div class="preloader-wrapper">
        <div class="preloader-circle-2"></div>
      </div>
      <div class="preloader-wrapper">
        /
        <div class="preloader-circle-3"></div>
      </div>
    </div>

    <!--- My Modal Screen ---->
    <div class="modal" id="modal">
      <span class="material-symbols-rounded" id="closeModal"> close </span>
      <img class="modal-image" id="modal-image" />
      <div id="modal-caption"></div>
    </div>

    <!--- Side Navigation ---->
    <div class="side-nav" id="side-nav">
      <a href="about-me.html"
        ><img
          src="https://drive.google.com/uc?export=view&id=1NSUoxeb29qUtJuw3QkOa-OBE75ATv0uQ"
          alt=""
      /></a>
      <a asp-area="" asp-controller="Home" asp-action="Index" class="owner-name"><h1>Axl Rich</h1></a>
      <a asp-area="" asp-controller="Home" asp-action="Index" class="nav-select">Home</a>
      <a asp-area="=" asp-controller="Home" asp-action="MyServices" class="nav-select">My Services</a>
      <!-- <partial name="_LoginPartial"/> -->

      <div class="socmed-buttons">
        <a class="fb-button" href="https://www.facebook.com/axlrich.fb/"
          ><i class="bi bi-facebook layout-logo"></i>
        </a>
        <a class="ig-button" href="https://www.instagram.com/axlrich.ig/"
          ><i class="bi bi-instagram layout-logo"></i
        ></a>
        <a class="tw-button" href="https://twitter.com/axlrichtwits"
          ><i class="bi bi-twitter layout-logo"></i
        ></a>
      </div>
    </div>
    <span
      class="material-symbols-rounded"
      id="closeNav"
      onclick="closeSideBar()"
    >
      close
    </span>
    <span class="material-symbols-rounded" id="openNav" onclick="openSideBar()">
      menu
    </span>
    

    <!--- Main Content ---->
    <div class="main-content" id="main-content">
      <!--- Main Content Header ---->
      <div class="header">
        <h1>@ViewData["Title"]</h1>
      </div>
      @RenderBody()

      <!--- Main Content Footer ---->
      <div class="main-footer">
        <div class="main-footer-container">
          <div class="recent-photos">
            <h4 style="align-self: center">Recent Photos</h4>
            <div class="footer-photos">
              <div class="photo portrait-photo">
                <img
                  src="https://drive.google.com/uc?export=view&id=1J-hIdHrqGnbzo3tRw30p_wbQUzvB-9lr"
                  alt="Clody Cabanlit"
                  onclick="displayModal(this)"
                />
              </div>
              <div class="photo portrait-photo">
                <img
                  src="https://drive.google.com/uc?export=view&id=1oLCIDYLHsOzXe1Je4H2oVwiw8JDfrPar"
                  alt="Joselito Villanueva"
                  onclick="displayModal(this)"
                />
              </div>
              <div class="photo portrait-photo">
                <img
                  src="https://drive.google.com/uc?export=view&id=1VhORoiT6swER_xF6fH3Vr9NnkDOGNU8X"
                  alt="Miana Mae Miparanum"
                  onclick="displayModal(this)"
                />
              </div>
              <div class="photo portrait-photo">
                <img
                  src="https://drive.google.com/uc?export=view&id=1R2FrAqnn4KF5WPvOtpC3cOy0YAUL6Xua"
                  alt="Our Siamese Cat"
                  onclick="displayModal(this)"
                />
              </div>
              <div class="photo portrait-photo">
                <img
                  src="https://drive.google.com/uc?export=view&id=176lpjfVYudETWwztbdZ682v9iTNkjjgH"
                  alt="Eric Miras"
                  onclick="displayModal(this)"
                />
              </div>
              <div class="photo portrait-photo">
                <img
                  src="https://drive.google.com/uc?export=view&id=1kEH132bnyqRv6tlu81INXtTEk5WqkI_V"
                  alt="Sniper 150 MXI(2016 Model)"
                  onclick="displayModal(this)"
                />
              </div>
            </div>
          </div>
          <div class="contact">
            <h4 style="align-self: center">Contact Me</h4>
            <div class="contact-info">
              <div class="info">
                <span class="material-symbols-rounded"> location_on </span>
                <span>Looc, Santander, Cebu, Philippines</span>
              </div>
              <div class="info">
                <span class="material-symbols-rounded"> call </span>
                <span>(+63) 929 621 2747</span>
              </div>
              <div class="info">
                <span class="material-symbols-rounded"> mail </span>
                <span>janaxlrichjuguilon@gmail.com</span>
              </div>
            </div>
          </div>
        </div>
        <p class="copyright">Copyright ©2023 All rights reserved.</p>
      </div>
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
    <script src="~/js/main.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
  </body>
</html>
